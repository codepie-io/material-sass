{
  "name": "@codepie/material-sass",
  "version": "1.0.0-beta.7",
  "description": "Material design framework built on latest css variables and follow BEM methodology. Give a native app look to your webapp instantly.",
  "main": "scss/material.scss",
  "style": "dist/css/material.css",
  "keywords": [
    "material design",
    "mobile-first",
    "responsive",
    "front-end",
    "material grid",
    "framework"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codepie-io/material-sass.git"
  },
  "scripts": {
    "compile": "gulp css",
    "watch": "gulp watch",
    "precommit": "lint-staged"
  },
  "homepage": "https://material.codepie.io",
  "author": "The codepie authors (https://github.com/codepie-io/material-sass/graphs/contributors)",
  "bugs": {
    "url": "https://github.com/codepie-io/material-sass/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "browserslist": "^4.16.1",
    "bundlesize": "^0.17.1",
    "css-mqpacker": "^7.0.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-notify": "^3.2.0",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-util": "^3.0.8",
    "prettier": "^2.2.1",
    "prettier-stylelint": "^0.4.2",
    "stylelint": "^10.0.1",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-order": "^3.1.1",
    "stylelint-scss": "^3.18.0",
    "stylelint-selector-bem-pattern": "^2.1.0"
  },
  "engines": {
    "node": ">=8"
  },
  "files": [
    "dist/",
    "scss/",
    "README.md"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 11"
  ],
  "bundlesize": [
    {
      "path": "./dist/css/material.css",
      "maxSize": "5 kB"
    }
